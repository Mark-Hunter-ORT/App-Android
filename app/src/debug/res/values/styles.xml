<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->


        <item name="colorPrimary">#C6E4F448</item>
        <item name="colorPrimaryDark">#C63D4215</item>
        <item name="colorAccent">#E2CDDC39</item>
        <item name="android:buttonStyle">@style/Button</item>
    </style>
    <style name="AppTheme.NoActionBar" parent="AppTheme">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Button" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:textColor">@color/fontColorclear</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily"></item>



    </style>

</resources>
